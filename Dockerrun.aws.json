{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "microservice-product",
      "image": "gustavo0426/microservice-product-h2:3.0.0",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5000
        }
      ],
      "environment": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/elasticbeanstalk/microservice-product",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "microservice"
        }
      }
    }
  ]
}
